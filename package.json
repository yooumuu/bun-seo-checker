{
  "name": "bun-seo-checker",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run server/index.ts",
    "dev": "bun --watch server/index.ts",
    "dev:full": "bunx concurrently \"bun --watch server/index.ts\" \"cd frontend && bunx --bun vite\"",
    "db:gen": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:push": "bunx drizzle-kit push",
    "db:studio": "bunx drizzle-kit studio"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "^9.1.1",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.19.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "hono": "^4.7.9",
    "playwright": "^1.56.1",
    "postgres": "^3.4.7",
    "zod": "^3.24.4"
  }
}
